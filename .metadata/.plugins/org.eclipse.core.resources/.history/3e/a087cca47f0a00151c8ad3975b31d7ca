package kr.co.taommall.common.controller;

import java.util.List;

import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpSession;

import kr.co.taommall.order.service.OrderService;
import kr.co.taommall.order.vo.Order;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.web.bind.annotation.RequestMapping;

public class OrderController {

		@Autowired
		OrderService service;
		
		@RequestMapping("/orderList.do")
		public String orderList(HttpServletRequest request,HttpSession session){
			
			List<Order> list = service.selectAllOrder();
			request.setAttribute("order_list", list);
			return "buyer/buyer_order_form";
		}
		
}
