package kr.co.taommall.note.service;

import java.util.List;

import org.springframework.ui.Model;

import kr.co.taommall.note.vo.Note;

public interface NoteService {

	void insertNote(Note note);
	void deleteNote(int noteNo);
	void updateNoteStore(int noteNo);
	void updateNoteUnStore(int noteNo);
	List<Note> selectReceiveNoteBuyer(Model model);
	List<Note> selectReceiveNoteSeller(Model model);
	List<Note> selectSendNoteBuyer(Model model);
	List<Note> selectSendNoteSeller(Model model);
	List<Note> selectStoreNoteBuyer(Model model);
	List<Note> selectStoreNoteSeller(Model model);

}
