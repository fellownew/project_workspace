package kr.co.taommall.board.service;

import kr.co.taommall.board.vo.Board;

import org.springframework.ui.Model;
import org.springframework.web.bind.annotation.ModelAttribute;

public interface BoardService {
	public String insertBoard(@ModelAttribute Board board,Model model);
	public String deleteBoard(@ModelAttribute Board board,Model model);
	public String selectBoardByProductId(@ModelAttribute Board board,Model model);
	
}
