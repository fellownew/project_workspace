		char[] i = { a,bn,c,d,e,f,g,h,i,j,k,l,m,n,o,p,q,r,s,t,u,v,w,x,y,z };
		request.setAttribute("list", i);%>
	<c:set var="a" value="${fn:length(list)}" />
	<c:forEach items="${list}" var="list" varStatus="status">
		<c:if test="${status.index % 7 == 0 and status.index != 0}">
			</ul>
		</c:if>
		<c:if test="${status.index % 7 == 0}">
			<ul>
		</c:if>
		<li><c:out value="${list}" /></li>
		<c:if test="${status.count == a}">
			</ul>
		</c:if>
