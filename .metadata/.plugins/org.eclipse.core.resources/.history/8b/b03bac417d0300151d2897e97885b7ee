package kr.co.taommall.product.dao;

import java.util.List;

import kr.co.taommall.product.vo.Product;

public interface ProductDAO {
	public int insertProduct(Product product);
	public int updateProduct(Product product);
	public int deleteProduct(String productId);
	public List<Product> selectAllProduct();
	public List<Product> selectProductById();
	public List<Product> selectProductByName();
	public List<Product> selectProductByCategory();
	public List<Product> selectProductByexpDate();
	public List<Product> selectProductBySellerId();
	public List<Product> selectProductByPrice();
	
}